    "type": "library",
    "keywords": [
        "zf2","entity","hydrator","contain"
    ],
    "homepage": "http://contain-project.org",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Andrew Kandels",
            "email": "akandels@gmail.com",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/andrew-kandels/contain-mapper/issues",
        "irc": "irc://irc.freenode.net/contain"
    },
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": ">=2.0.0",
        "akandels/contain": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-0": {
            "ContainMapper": "src/",
            "ContainMapperTest": "tests/"
        }
    }
}
